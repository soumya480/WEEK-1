#include<stdio.h>
int main(){
  int n,element;
  printf("Enter size of array: ");
  scanf("%d",&n);
  int array[n];
  printf("Enter elements of array:\n");
  for(int i=0;i<n;i++){
    scanf("%d",&array[i]);
  }
  printf("Enter the element to be inserted: ");
  scanf("%d",&element);
  array[n]=element;
  printf("Array after insertion: ");
  for(int i=0;i<=n;i++){
    printf("%d ",array[i]);
  }
  return 0;
  }
