#include <stdio.h>

int main() {
    int i, n;

    printf("Enter the size of the array: ");
    scanf("%d", &n);

    if (n < 2) {
        printf("Second largest element doesn't exist.\n");
        return 0;
    }

    int array[n];
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &array[i]);
    }

    int max_1, max_2;

    
    if (array[0] > array[1]) {
        max_1 = array[0];
        max_2 = array[1];
    } else {
        max_1 = array[1];
        max_2 = array[0];
    }

    for (i = 2; i < n; i++) {
        if (array[i] > max_1) {
            max_2 = max_1;
            max_1 = array[i];
        } else if (array[i] > max_2 && array[i] != max_1) {
            max_2 = array[i];
        }
    }

    if (max_1 == max_2) {
        printf("All elements are the same. No second largest.\n");
    } else {
        printf("The second largest element is: %d\n", max_2);
    }

    return 0;
}
