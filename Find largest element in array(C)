#include<stdio.h>
int main(){
  int i,n;
 
  printf("Enter the size of array:");
  scanf("%d",&n);
  int array[n];
  printf("Enter the elements of array:\n");
  for(i=0;i<n;i++){
    scanf("%d",&array[i]);
  }
  int max_element = array[0];
  for(i=0;i<n;i++){
    if(array[i]>max_element){
      max_element = array[i];
    }
  }
  printf("The maximum element in the array is: %d",max_element);
  return 0;
}
